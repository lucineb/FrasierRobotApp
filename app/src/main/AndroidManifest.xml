<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.frasier.frasierapplication" >

    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.frasier.frasierapplication.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.frasier.frasierapplication.Calendar"
            android:label="@string/calendar" >
        </activity>
        <activity
            android:name="com.example.frasier.frasierapplication.Vitals"
            android:label="@string/vitals" >
        </activity>
        <activity
            android:name="com.example.frasier.frasierapplication.Follow"
            android:label="@string/follow" >
        </activity>
        <activity
            android:name="com.example.frasier.frasierapplication.Arm"
            android:label="@string/arm" >
        </activity>
        <activity
            android:name="com.example.frasier.frasierapplication.Emergency"
            android:label="@string/emergency" >
        </activity>
    </application>

</manifest>